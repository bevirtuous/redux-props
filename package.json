  "version": "1.0.0-beta.3",
  "license": "MIT",
  "description": "Consume a Redux store inside React components.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist/ && NODE_ENV=production babel src --out-dir dist --no-comments",
    "release": "npm run build && git add -A",
